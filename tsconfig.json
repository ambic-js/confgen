{
  "compilerOptions": {
    "noImplicitAny": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": { "~/*": ["lib/*"] },
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es2017"],
    "downlevelIteration": true,
    "outDir": "dist"
  },
  "include": ["lib"]
}
