{
  "name": "configgen",
  "bin": {
    "configgen": "./dist/index.umd.js"
  },
  "scripts": {
    "config": "npx ts-node ./src/index.ts",
    "configgen": "npm run build; ./dist/index.umd.js vite library:Configgen node:fs:child_process prettier",
    "bump": "./bump.sh",
    "build": "npm run build:vite; npm run build:bin",
    "build:vite": "vite build",
    "build:bin": "echo '#!/usr/bin/env node'|cat - dist/index.umd.js > /tmp/out && mv /tmp/out dist/index.umd.js; chmod a+x dist/index.umd.js",
    "fix:format": "prettier --write ."
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "merge-objects": "^1.0.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/merge-objects": "^1.0.0",
    "prettier": "*",
    "ts-node": "^10.5.0",
    "vite": "*",
    "vite-plugin-commonjs-externals": "*"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  }
}
